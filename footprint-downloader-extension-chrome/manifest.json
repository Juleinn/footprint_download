{"name": "footprint-downloader",
 "description": "A footprint and symbols downloader for KiCAD",
 "version": "0.1",
 "background": {"scripts": ["bg.js"], "persistent": false},
 "options_page": "options.html",
 "permissions": ["downloads", "tabs"],
"icons":{"32":"icon.png"},
"content_scripts": [{
"matches": ["*://*/*"],
"js": ["inject.js"]
  }],
 "manifest_version": 2}
