{"name": "footprint-downloader",
 "description": "A footprint and symbols downloader for KiCAD",
 "version": "0.1",
 "background": {"scripts": ["bg.js"], "persistent": false},
 "permissions": ["downloads", "tabs",   "<all_urls>" ],
"icons":{"32":"icon.png"},
"content_scripts": [{
    "all_frames":true,
"matches": ["*://*/*", "<all_urls>"],
"js": ["inject.js"]
  }],
 "manifest_version": 2}
